local library = loadstring(game.HttpGet(game, "https://raw.githubusercontent.com/bloodball/-back-ups-for-libs/main/0x"))()

local w1 = library:Window("Impossible Glass Bridge") -- Text

setclipboard("https://discord.io/FrankBlox")
game.StarterGui:SetCore("SendNotification", {
    Title = "Credits: FrankBlox Scripts",
    Text = "Discord Link Has Copied",
    Duration = 500,
    })

    w1:Button(
        "Show Correct Glass",
        function()
            for i,v in pairs(game:GetService("Workspace")["Glass Bridge"].GlassPane:GetDescendants()) do
                if v:IsA("BasePart") then
                    if v.CanCollide == true then
                    v.Color = Color3.fromRGB(0, 255, 0)
                    else
                  
                  v.Color = Color3.fromRGB(255, 0, 0)
                    
                    end
                end
                end
        end
    ) -- Text, Callback


w1:Label("YT: FrankBlox Scripts") -- Text
